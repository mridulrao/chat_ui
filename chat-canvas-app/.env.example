# Copy this file to .env and fill in your project-specific values

# Supabase (required for auth)
VITE_SUPABASE_URL="https://YOUR_PROJECT_ID.supabase.co"
VITE_SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_PUBLIC_KEY"

# Optional: LLM proxy for ChatCanvas streaming (already wired via Vite dev proxy)
# If you keep the included Express server, you can leave this empty in development.
# VITE_LLM_PROXY_URL="http://localhost:8787/api/chat/stream"

# Optional: Model alias; server maps gpt40-mini -> gpt-4o-mini
VITE_LLM_MODEL="gpt40-mini"
